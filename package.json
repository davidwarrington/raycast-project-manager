{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "project-manager",
  "title": "Project Manager",
  "description": "Open projects in your code editor from Raycast",
  "icon": "command-icon.png",
  "author": "davidwarrington",
  "license": "MIT",
  "commands": [
    {
      "name": "open-project",
      "title": "Open Project",
      "description": "",
